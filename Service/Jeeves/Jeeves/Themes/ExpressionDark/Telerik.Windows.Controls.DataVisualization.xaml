    <Style x:Key="RadTimeBarStyle" TargetType="datavizControls:RadTimeBar">
        <Setter Property="Foreground" Value="{StaticResource TimeBar_Foreground}"/>
        <Setter Property="Background" Value="{StaticResource TimeBar_Background}"/>
