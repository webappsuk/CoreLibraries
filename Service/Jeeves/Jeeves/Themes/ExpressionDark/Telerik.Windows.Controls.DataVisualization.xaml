    <Style x:Key="TimelineItemContainerStyle" TargetType="timelines:TimelineItemContainer">
        <Setter Property="Canvas.ZIndex" Value="200"/>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <timelines:TimelineItemGroupsPanel Padding="0 43 0 5" VerticalScrollBar="{Binding ElementName=Part_VerticalSlider}"/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
