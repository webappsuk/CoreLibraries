                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="timelines:TimelineStripLineControl" BasedOn="{StaticResource TimelineStripLineControlStyle}"/>
    <Style TargetType="timelines:TimelinePeriodControl" BasedOn="{StaticResource TimelinePeriodControlStyle}"/>
    <Style TargetType="timelines:TimelineGroupPeriodControl" BasedOn="{StaticResource TimelineGroupPeriodControlStyle}"/>
    <Style x:Key="TimelineItemGroupControlToggleButtonStyle" TargetType="telerikControls:RadToggleButton">
        <Setter Property="BorderThickness" Value="{StaticResource Timeline_TimelineItemGroup_ToggleButton_OuterBorder_Thickness}"/>
        <Setter Property="BorderBrush" Value="{StaticResource Timeline_TimelineItemGroup_ToggleButton_OuterBorder_BorderBrush}"/>
        <Setter Property="Background" Value="{StaticResource Timeline_TimelineItemGroup_ToggleButton_OuterBorder_Background}"/>
        <Setter Property="CornerRadius" Value="{StaticResource Timeline_TimelineItemGroup_ToggleButton_OuterBorder_CornerRadius}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControls:RadToggleButton">
                    <Grid Background="#01FFFFFF">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="buttonBorder" Storyboard.TargetProperty="BorderBrush" Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{StaticResource Timeline_TimelineItemGroup_ToggleButton_OuterBorder_BorderBrush_MouseOver}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="buttonBorder" Storyboard.TargetProperty="Background" Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{StaticResource Timeline_TimelineItemGroup_ToggleButton_OuterBorder_Background_MouseOver}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="expandButton" Storyboard.TargetProperty="Fill" Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{StaticResource Timeline_TimelineItemGroup_ToggleButton_Button_Fill_MouseOver}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="collapseButton" Storyboard.TargetProperty="Fill" Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{StaticResource Timeline_TimelineItemGroup_ToggleButton_Button_Fill_MouseOver}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="buttonBorder">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="DisabledChecked">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="buttonBorder">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="CheckStates">
                                <VisualState x:Name="Checked">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0" Storyboard.TargetName="collapseButton" Storyboard.TargetProperty="Opacity" To="1"/>
                                        <DoubleAnimation Duration="0" Storyboard.TargetName="expandButton" Storyboard.TargetProperty="Opacity" To="0"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Unchecked"/>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <StackPanel Orientation="Horizontal">
                            <Border x:Name="buttonBorder"
                                    Width="{StaticResource Timeline_TimelineItemGroup_ToggleButton_OuterBorder_Width}"
                                    Height="{StaticResource Timeline_TimelineItemGroup_ToggleButton_OuterBorder_Height}"
                                    Margin="3,0,0,0"
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    Background="{TemplateBinding Background}"
                                    CornerRadius="{TemplateBinding CornerRadius}"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center">
                                <Grid>
                                    <Path x:Name="expandButton"
                                            Width="{StaticResource Timeline_TimelineItemGroup_ToggleButton_Button_Width}"
                                            Height="{StaticResource Timeline_TimelineItemGroup_ToggleButton_ExpandButton_Height}"
                                            Fill="{StaticResource Timeline_TimelineItemGroup_ToggleButton_Button_Fill}"
                                            Data="M2,0 L3,0 3,2 5,2 5,3 3,3 3,5 2,5 2,3 0,3 0,2 2,2 z"
                                            Opacity="1"/>
                                    <Rectangle x:Name="collapseButton"
                                            Width="{StaticResource Timeline_TimelineItemGroup_ToggleButton_Button_Width}"
                                            Height="{StaticResource Timeline_TimelineItemGroup_ToggleButton_CollapseButton_Height}"
                                            VerticalAlignment="Center"
                                            HorizontalAlignment="Center"
                                            Fill="{StaticResource Timeline_TimelineItemGroup_ToggleButton_Button_Fill}"
                                            Opacity="0"/>
                                </Grid>
                            </Border>
                            <TextBlock Text="{TemplateBinding Content}" Margin="4 0 0 0"/>
                        </StackPanel>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="TimelineItemGroupControlStyle" TargetType="timelines:TimelineItemGroupControl">
        <Setter Property="FontFamily" Value="{StaticResource TimelineFontFamily}"/>
        <Setter Property="FontSize" Value="{StaticResource Timeline_TimelineItemGroup_FontSize}"/>
        <Setter Property="Foreground" Value="{StaticResource Timeline_TimelineItemGroup_Foreground}"/>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <timelines:VirtualizingTimelinePanel Slider="{Binding ElementName=PART_Slider}" Background="#01FFFFFF"/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Header" Value="{Binding GroupKey}"/>
        <Setter Property="ItemsSource" Value="{Binding DataItems}"/>
        <Setter Property="ItemTemplateSelector" Value="{Binding Path=ItemTemplateSelector, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type datavizControls:RadTimeline}}}"/>
        <Setter Property="DataGroups" Value="{Binding DataGroups}"/>
        <Setter Property="ExpandMode" Value="{Binding GroupExpandMode, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=datavizControls:RadTimeline}}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
        <Setter Property="IsTabStop" Value="false"/>
        <Setter Property="animation:AnimationManager.AnimationSelector">
            <Setter.Value>
                <animation:AnimationSelector>
                    <animation:ExpanderExpandCollapseAnimation AnimationName="Expand" Direction="In" TargetElementName="Content"/>
                    <animation:ExpanderExpandCollapseAnimation AnimationName="Collapse" Direction="Out" TargetElementName="Content"/>
                    <timelines:TimelineResizeAnimation AnimationName="Resize" TargetElementName="Content"/>
                </animation:AnimationSelector>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="timelines:TimelineItemGroupControl">
                    <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid x:Name="Header"
                                    Grid.Row="0"
                                    Margin="0 5 0 0"
                                    Visibility="{Binding Path=HasHeader, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource booleanToVisibilityConverter}}">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <telerikControls:RadToggleButton x:Name="HeaderButton"
                                        Grid.Column="0"
                                        Content="{TemplateBinding Header}"
                                        ContentTemplate="{TemplateBinding HeaderTemplate}"
                                        Style="{StaticResource TimelineItemGroupControlToggleButtonStyle}"
                                        IsChecked="{Binding IsExpanded, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                                        IsEnabled="{Binding ExpandMode, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource groupExpandModeToBooleanConverter}}"/>
                                <Path
                                        Grid.Column="1"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Stretch"
                                        Data="M0,0 L800,0"
                                        Height="{StaticResource Timeline_TimelineItemGroup_Separator_Height}"
                                        Margin="5 1 0 0"
                                        Stretch="Fill"
                                        Stroke="{StaticResource Timeline_TimelineItemGroup_Separator_Stroke}"/>
                            </Grid>
                            <ItemsPresenter x:Name="Content" Grid.Row="1"/>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="timelines:TimelineItemGroupControl" BasedOn="{StaticResource TimelineItemGroupControlStyle}"/>
    <Style x:Key="TimelinePeriodHeaderStyle" TargetType="Border">
        <Setter Property="Height" Value="23"/>
        <Setter Property="VerticalAlignment" Value="Top"/>
    </Style>
    <Style x:Key="TimelineGroupPeriodHeaderStyle" TargetType="Border">
        <Setter Property="Height" Value="15"/>
        <Setter Property="VerticalAlignment" Value="Top"/>
    </Style>
    <Style x:Key="TimelineLineStyle" TargetType="Border">
        <Setter Property="BorderBrush" Value="{StaticResource Timeline_Button_Bottom_BorderBrush}"/>
        <Setter Property="BorderThickness" Value="{StaticResource Timeline_Button_Bottom_Thickness}"/>
        <Setter Property="Background" Value="{StaticResource Timeline_Button_Bottom_Background}"/>
        <Setter Property="Height" Value="4"/>
    </Style>
    <Style x:Key="TimelineItemControlStyle" TargetType="timelines:TimelineItemControl">
        <Setter Property="Height" Value="10"/>
        <Setter Property="BorderBrush" Value="{StaticResource Timeline_TimelineItem_Stroke}"/>
        <Setter Property="Margin" Value="0 5 0 0"/>
        <Setter Property="Background" Value="{StaticResource Timeline_TimelineItem_Fill}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="timelines:TimelineItemControl">
                    <Border Background="#01FFFFFF">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem" Storyboard.TargetProperty="Fill" Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{StaticResource Timeline_TimelineItem_Fill_MouseOver}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem" Storyboard.TargetProperty="Stroke" Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{StaticResource Timeline_TimelineItem_Stroke_MouseOver}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem" Storyboard.TargetProperty="Fill" Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{StaticResource Timeline_TimelineItem_Fill_Pressed}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem" Storyboard.TargetProperty="Stroke" Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{StaticResource Timeline_TimelineItem_Stroke_Pressed}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Rectangle x:Name="timelineItem" Margin="0 .5 0 .5" Fill="{TemplateBinding Background}" Stroke="{TemplateBinding BorderBrush}"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="TimelineInstantItemControlStyle" TargetType="timelines:TimelineInstantItemControl">
        <Setter Property="Margin" Value="0 5 0 0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="timelines:TimelineInstantItemControl">
                    <Border Background="#01FFFFFF" Width="10" Height="10">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem" Storyboard.TargetProperty="Fill" Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{StaticResource Timeline_TimelineInstantItem_Fill_MouseOver}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem" Storyboard.TargetProperty="Stroke" Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{StaticResource Timeline_TimelineInstantItem_Stroke_MouseOver}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem" Storyboard.TargetProperty="Fill" Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{StaticResource Timeline_TimelineInstantItem_Fill_Pressed}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem" Storyboard.TargetProperty="Stroke" Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{StaticResource Timeline_TimelineInstantItem_Stroke_Pressed}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Rectangle x:Name="timelineItem"
                                Height="7"
                                Width="7"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                StrokeThickness="{StaticResource Timeline_TimelineInstantItem_Stroke_Thickness}"
                                Fill="{StaticResource Timeline_TimelineInstantItem_Fill}">
                            <Rectangle.RenderTransform>
                                <RotateTransform Angle="45" CenterX="3.5" CenterY="3.5"/>
                            </Rectangle.RenderTransform>
                        </Rectangle>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <DataTemplate x:Key="TimelineItemTemplate">
        <timelines:TimelineItemControl/>
    </DataTemplate>
    <DataTemplate x:Key="TimelineInstantItemTemplate">
        <timelines:TimelineInstantItemControl/>
    </DataTemplate>
    <Style x:Key="TimelineAnnotationStyle" TargetType="timelines:TimelineAnnotation">
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="Margin" Value="{StaticResource Timeline_TimelineAnnotation_Margin}"/>
        <Setter Property="Canvas.ZIndex" Value="300"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="timelines:TimelineAnnotation">
                    <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="SelectionRegionStyle" TargetType="timelines:SelectionRegion">
        <Setter Property="Visibility" Value="Collapsed"/>
        <Setter Property="Background" Value="{StaticResource Timeline_SelectionRegion_Fill}"/>
        <Setter Property="BorderBrush" Value="{StaticResource Timeline_SelectionRegion_Stroke}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="timelines:SelectionRegion">
                    <Rectangle Stroke="{TemplateBinding BorderBrush}" StrokeDashArray="3 3" Fill="{TemplateBinding Background}"/>
                </ControlTemplate>
