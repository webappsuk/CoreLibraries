                    MinHeight="22"
                    Background="{StaticResource GridView_ValidationTooltipBackground}"
                    BorderBrush="{StaticResource GridView_ValidationTooltipOuterBorder}"
                    CornerRadius="{StaticResource GridView_ValidationTooltipCornerRadius}"
