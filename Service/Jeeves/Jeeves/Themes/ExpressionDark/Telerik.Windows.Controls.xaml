                            <Setter TargetName="InnerBorder" Property="BorderBrush" Value="{StaticResource ControlInnerBorder_Selected}"/>
                            <Setter TargetName="OuterBorder" Property="Background" Value="{StaticResource ControlBackground_Selected}"/>
