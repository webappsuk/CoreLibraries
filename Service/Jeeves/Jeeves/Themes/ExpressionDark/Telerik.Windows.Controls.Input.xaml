                    Visibility="{Binding IsReadOnly, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}"/>
            <telerik:RadWatermarkTextBox x:Name="PART_DateTimeInput"
